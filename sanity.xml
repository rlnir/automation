<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="sanity" verbose="2" thread-count="3" parallel="classes">

    <groups>
        <run>
            <include name="sanity"/>
        </run>
    </groups>
    <test name="sanity">
        <packages>
            <package name="com.seleniumAssignment.*"/>
        </packages>
    </test>
</suite>